@Library('jenkins-shared-libraries@gelo') _

pipeline {
    agent {
      kubernetes { yaml pipelineGetAgent(devopscli_version: 'latest') }
    }

    stages{
        stage('Test Container') {
            steps {
                container('devops-cli') {
                    sh "python3 main.py init"
                    sh "python3 main.py assume"
                }
            }
        }
    }
}