@Library('jenkins-shared-libraries@gelo') _

pipeline {
    
    agent {
      kubernetes { yaml pipelineGetAgent(alpine_version: 'latest') }
    }

    stages{
        stage('Test Container') {
            steps {
                container('alpine') {
                    sh "aws sts get-caller-identity"
                    sh "python --version"
                    sh "main.py"
                }
            }
        }
    }
}